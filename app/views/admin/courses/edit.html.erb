<h2>Edit Course</h2>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for [:admin, @course] do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name, class: "form-control" %>
      <br>
      <%= f.submit "Edit Course", class: "btn btn-primary" %>

    <% end %>

  </div>
</div>
